<?xml version="1.0" encoding="utf-8"?>
<paths xmlns:android="http://schemas.android.com/apk/res/android">
    <!--
    file-paths
        表示Context.getFilesDir()目录或者其子目录。
        示例 : /data/data/com.chen.gradle/files/test1/name1
        -->
    <files-paths
        name="name1"
        path="test1" />

    <!--
    cache-path
        表示Context.getCacheDir()目录或者其子目录。
        示例 : /data/data/com.chen.gradle/cache/test2/name2
        -->
    <cache-path
        name="name2"
        path="test2" />

    <!--
    external-path
        表示Environment.getExternalStorageDirectory()目录或者其子目录。
        示例 : /storage/emulated/0/test3/name3
        -->
    <external-path
        name="nhdz"
        path="Download" />

    <!--
    external-files-path
        表示Context.getExternalFilesDir(null)目录或者其子目录。
        示例 : /storage/emulated/0/Android/data/com.chen.gradle/files/test4/name4
        -->
    <external-files-path
        name="name4"
        path="test4" />


    <!--
     external-cache-path
        表示Context.getExternalCacheDir()目录或者其子目录。
        示例 : /storage/emulated/0/Android/data/com.chen.gradle/cache/test5/name5
        -->
    <external-cache-path
        name="name5"
        path="test5" />

</paths>